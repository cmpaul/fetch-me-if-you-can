<h2>Show job</h2>

<ul>

  <li>
    <strong>Job:</strong>
    <%= @job.job_id %>
  </li>

  <li>
    <strong>URL:</strong>
    <%= @job.url %>
  </li>

  <li>
    <strong>Status:</strong>
    <%= @job.status %>
  </li>

  <li>
    <strong>Title:</strong>
    <%= @job.title %>
  </li>

  <li>
    <strong>Content:</strong>
    <%= @job.content %>
  </li>

</ul>

<%= link "Edit", to: job_path(@conn, :edit, @job) %>
<%= link "Back", to: job_path(@conn, :index) %>
